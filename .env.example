# Environment Variables Template
# Copy this to .env and fill in your values

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/evolvx"

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=
CLERK_WEBHOOK_SECRET=

# GitHub App
GITHUB_APP_ID=
GITHUB_APP_NAME=evolvx-ai-deployer
GITHUB_APP_CLIENT_ID=
GITHUB_APP_CLIENT_SECRET=
GITHUB_APP_PRIVATE_KEY=
GITHUB_APP_WEBHOOK_SECRET=

# Polar Billing
POLAR_ACCESS_TOKEN=
POLAR_ORGANIZATION_ID=
POLAR_WEBHOOK_SECRET=
POLAR_PRICE_ID_PRO=
POLAR_PRICE_ID_TEAM=

# OpenAI
OPENAI_API_KEY=

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# Inngest
INNGEST_EVENT_KEY=
INNGEST_SIGNING_KEY=

# Application
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
API_URL=http://localhost:3001
PORT=3001
HOST=0.0.0.0
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# S3 Storage (MinIO or AWS)
S3_ENDPOINT=https://s3.eu-north-1.amazonaws.com
S3_ACCESS_KEY=your-aws-access-key-id
S3_SECRET_KEY=your-aws-secret-access-key
S3_BUCKET=evolvx-artifacts
S3_REGION=eu-north-1    

# Runner Service
RUNNER_DOCKER_HOST=unix:///var/run/docker.sock
RUNNER_MAX_BUILD_TIME=600000

# Monitoring
MONITORING_INTERVAL=30000
